# CONFIGURAR UTILIZAÇÃO DO HTTPS
RewriteEngine On
RewriteCond %{SERVER_PORT} 80
RewriteRule ^(.*)$ https://www.grupovolpato.com/$1 [R,L]

RewriteRule ^a-volpato/?$ /a-volpato.php [NC,L]
RewriteRule ^contato/?$ /contato.php [NC,L]
RewriteRule ^seguranca-eletronica/?$ /seguranca-eletronica.php [NC,L]
RewriteRule ^projetos-integrados/?$ /projetos-integrados.php [NC,L]
RewriteRule ^area-cliente/?$ /area-cliente.php [NC,L]
RewriteRule ^segunda-via-boleto/?$ /segunda-via-boleto.php [NC,L]
RewriteRule ^cliente-alarme/?$ /cliente-alarme.php [NC,L]
RewriteRule ^politica-de-privacidade/?$ /politica-de-privacidade.php [NC,L]